module body Example.Fibonacci is
    function Fibonacci(n: Integer_32): Integer_32 is
        if n < 2 then
            return n;
        else
            return Fibonacci(n - 1) + Fibonacci(n - 2);
        end if;
    end;

    function Main(): Unit is
        Fibonacci(30);
        return nil;
    end;
end module body.
