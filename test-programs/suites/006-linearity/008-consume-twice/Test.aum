module body Test is
    record R : Linear is
        x: Int32;
    end;

    function Consume(r: R): Unit is
        let { x: Int32 } := r;
        return nil;
    end;

    function Main(root: Root_Capability): Root_Capability is
        let r: R := R(x => 32);
        Consume(r);
        Consume(r);
        return root;
    end;
end module body.
